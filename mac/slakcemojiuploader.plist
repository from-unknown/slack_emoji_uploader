<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>SlackEmojiUploader</string>
        <key>ProgramArguments</key>
        <array>
          <string>/bin/bash</string>
          <string>-l</string>
          <string>-c</string>
	        <string>/PATH_TO_FOLDER/slack_emoji_uploader/mac/slack_emoji_uploader.sh</string>
        </array>
        <!-- launchctl loadしたタイミングで実行するか -->
        <key>RunAtLoad</key>
          <true/>
        <key>StartInterval</key>
          <integer>10</integer>
    </dict>
</plist>
